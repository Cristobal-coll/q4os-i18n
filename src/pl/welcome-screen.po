msgid "<p>Do you want to turn Desktop Effects on ?</p>"
msgstr "<p>Czy chcesz włączyć Efekty Pulpitu?</p>"

#: src/form1.cpp:126 src/form1.cpp:134 src/form1.cpp:138
msgid "Desktop effects"
msgstr "Efekty Pulpitu"

#: src/form1.cpp:126 src/form1.cpp:128
msgid "Turn ON"
msgstr "Włącz"

#: src/form1.cpp:126 src/form1.cpp:128 src/form1.cpp:171
